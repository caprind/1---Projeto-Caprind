Private Sub ProcSalvar()
On Error GoTo tratar_erro

If USMsgBox("Deseja realmente recalcular os dados desta ordem e OS?", vbYesNo, "CAPRIND v5.0") = vbYes Then
    With msk_fltFim
        If FunVerificaDataFinal(msk_fltInicio.Value, .Value) = False Then
            .Value = Date
            .SetFocus
            Exit Sub
        End If
    End With
    
    Data = msk_fltInicio.Value
    DataConclusaoOrdem = msk_fltFim.Value
    With frmprod
        .ProcAtualizaDadosOSOrdem
        USMsgBox ("Recalculo efetuado com sucesso."), vbInformation, "CAPRIND v5.0"
        '==================================
        Modulo = "PCP/Gerenciamento de ordem"
        Evento = "Recalcular ordem e OS"
        ID_documento = .txtof.Text
        Documento = "Ordem: " & .txtof.Text
        Documento1 = "OS: " & .cmbAPOS.Text
        ProcGravaEvento
        '==================================
    End With
    Unload Me
End If

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
On Error GoTo tratar_erro

Select Case KeyCode
    Case vbKeyEscape: Unload Me
    Case vbKeyF3: ProcSalvar
End Select

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

