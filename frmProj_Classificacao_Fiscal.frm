    End Select
    StrSqlLocCF = "Select * FROM tbl_ClassificacaoFiscal where " & TextoFiltro & FunVerifTipoFiltroIMF(Optinicio, Optmeio, Optfim, optIgual, txtTexto) & " and " & TextoFiltroPadrao
Else
    StrSqlLocCF = "Select * FROM tbl_ClassificacaoFiscal where " & TextoFiltroPadrao
End If
