                                    .Txt_ID_CF_cliente = lstClassFiscal.SelectedItem
                                    .Txt_CF_cliente = lstClassFiscal.SelectedItem.ListSubItems(2)
                                Else
                                    .Txt_ID_CF = lstClassFiscal.SelectedItem
                                    .Txt_CF = lstClassFiscal.SelectedItem.ListSubItems(2)
                                End If
                            End With
End If
Unload Me

Exit Sub
