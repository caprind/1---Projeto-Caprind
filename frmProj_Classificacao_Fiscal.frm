                        With frmSubstituicao_Tributaria
                            .Txt_ID_CF = lstClassFiscal.SelectedItem
                            .Txt_CF = lstClassFiscal.SelectedItem.ListSubItems(2)
                        End With
                    ElseIf Familia_NCM = True Then
                            With frmproj_familia
                                .Txt_ID_CF = lstClassFiscal.SelectedItem
                                .Txt_CF = lstClassFiscal.SelectedItem.ListSubItems(2)
                            End With
                        Else
                            With frmproj_produto
