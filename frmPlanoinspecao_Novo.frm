Private Sub ProcFiltrar()
On Error GoTo tratar_erro

CamposFiltro = "P.codProduto, P.Desenho, P.Descricao, P.Unidade, P.classe, F.Fase, F.Maquina, CM.Descricao as Descricao_posto, F.Grupo_op, F.IDFASE, F.Versao"
INNERJOINTEXTO = "Select " & CamposFiltro & " from ((((Projproduto P LEFT JOIN item_aplicacoes IA ON IA.codproduto = P.codproduto) LEFT JOIN Processos PR ON P.Codproduto = PR.Codproduto) LEFT JOIN Fases F ON F.IDprocesso = PR.IDprocesso) LEFT JOIN CadMaquinas CM ON CM.Maquina = F.Maquina) LEFT JOIN Projproduto_fabricante PFAB ON PFAB.Codproduto = P.codproduto"

If Chk_materia_prima.Value = 1 Or chkSemProcesso.Value = 1 Then
    TextoFiltroProc = " and PR.IDprocesso IS NULL"
    If Chk_materia_prima.Value = 1 Then TextoFiltroFase = " and P.Plano_inspecao = 'False' and P.Subtipoitem = 0" Else TextoFiltroFase = " and P.Plano_inspecao = 'False' and P.Processo = 'False'"
Else
    TextoFiltroProc = " and PR.IDprocesso IS NOT NULL and PR.Revisao = (Select MAX(PR1.Revisao) from Processos PR1 where PR1.IDprocesso = PR.IDprocesso)"
    TextoFiltroFase = " and F.Plano_inspecao = 'False'"
End If
'TextoFiltroPadrao = "P.Qualidade = 'True' and P.Tipo = 'P'" & TextoFiltroProc & TextoFiltroFase & " group by P.codProduto, P.Desenho, P.Descricao, P.Unidade, P.classe, F.Fase, F.Maquina, CM.Descricao, F.Grupo_op, F.IDFASE, F.Versao order by P.Desenho, F.Fase, F.Versao"
TextoFiltroPadrao = "P.Tipo = 'P'" & TextoFiltroProc & TextoFiltroFase & " group by P.codProduto, P.Desenho, P.Descricao, P.Unidade, P.classe, F.Fase, F.Maquina, CM.Descricao, F.Grupo_op, F.IDFASE, F.Versao order by P.Desenho, F.Fase, F.Versao"

If txtTexto.Visible = True And txtTexto <> "" Or cmbfamilia.Visible = True And cmbfamilia <> "" Then
    If cmbfiltrarpor = "Família" Then
        StrSqlLocProdPadrao = INNERJOINTEXTO & " where classe = '" & cmbfamilia & "' and " & TextoFiltroPadrao
    Else
        Select Case cmbfiltrarpor
            Case "Código interno": TextoFiltro = "P.desenho"
            Case "Descrição": TextoFiltro = "P.descricao"
            Case "Código de referência": TextoFiltro = "IA.N_referencia"
            Case "Part number": TextoFiltro = "PFAB.Part_number"
        End Select
        StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltro & FunVerifTipoFiltroIMF(Optinicio, Optmeio, Optfim, optIgual, txtTexto) & " and " & TextoFiltroPadrao
    End If
Else
    StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltroPadrao
End If
'Debug.print StrSqlLocProdPadrao

ProcCarregaLista

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

