Private Sub cmbfiltrarpor_Click()
On Error GoTo tratar_erro

ListView1.ListItems.Clear
If (cmbfiltrarpor = "Ordem" Or cmbfiltrarpor = "OS") And txtTexto <> "" Then
    VerifNumero = txtTexto
    ProcVerificaNumero
    If VerifNumero = False Then
        txtTexto = ""
        txtTexto.SetFocus
        Exit Sub
    End If
End If

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub ProcFiltrar()
On Error GoTo tratar_erro

ListView1.ListItems.Clear
If txtTexto.Visible = True And txtTexto <> "" Then
    If cmbfiltrarpor = "Ordem" Or cmbfiltrarpor = "OS" Then
        If cmbfiltrarpor = "Ordem" Then TextoFiltro = "producao.Ordem" Else TextoFiltro = "ordemservico.idproducao"
        StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem where producao.Ordem = " & txtTexto.Text & " order by producao.Ordem desc"
    Else
        Select Case cmbfiltrarpor
            Case "Código interno": TextoFiltro = "producao.desenho"
            Case "Descrição": TextoFiltro = "producao.produto"
            Case "Cliente": TextoFiltro = "producao.cliente"
        End Select
        If Optinicio.Value = True Then StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem where " & TextoFiltro & " like '" & txtTexto.Text & "%' order by producao.Ordem desc"
        If Optmeio.Value = True Then StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem where " & TextoFiltro & " like '%" & txtTexto.Text & "%' order by producao.Ordem desc"
        If Optfim.Value = True Then StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem where " & TextoFiltro & " like '%" & txtTexto.Text & "' order by producao.Ordem desc"
        If optIgual.Value = True Then StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem where " & TextoFiltro & " = '" & txtTexto.Text & "' order by producao.Ordem desc"
    End If
Else
    StrSql_SD_Ordem = "Select ordemservico.* from Producao INNER JOIN ordemservico on producao.Ordem = ordemservico.Ordem order by producao.Ordem desc"
End If
ProcCarregaLista

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

