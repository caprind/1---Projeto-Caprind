Sub ProcCarregaLista()
On Error GoTo tratar_erro

IDRequisicao = 0
Set TBLISTA = CreateObject("adodb.recordset")
TBLISTA.Open StrSql_SD_Ordem, Conexao, adOpenKeyset, adLockOptimistic
If TBLISTA.EOF = False Then
    TBLISTA.MoveLast
    PBLista.Min = 0
    PBLista.Max = TBLISTA.RecordCount
    PBLista.Value = 1
    contador = 0
    TBLISTA.MoveFirst
    Do While TBLISTA.EOF = False
        With ListView1.ListItems
            .Add , , IIf(IsNull(TBLISTA!Ordem), "", TBLISTA!Ordem)
            .Item(.Count).SubItems(1) = IIf(IsNull(TBLISTA!IDProducao), "", TBLISTA!IDProducao)
            .Item(.Count).SubItems(2) = IIf(IsNull(TBLISTA!Fase), "", TBLISTA!Fase)
            Set TBFases = CreateObject("adodb.recordset")
            TBFases.Open "Select Grupo_op from Fases where idfase = " & TBLISTA!IDFase, Conexao, adOpenKeyset, adLockOptimistic
            If TBFases.EOF = False Then
                .Item(.Count).SubItems(3) = IIf(IsNull(TBFases!Grupo_op), "", TBFases!Grupo_op)
            End If
            TBFases.Close
            .Item(.Count).SubItems(4) = IIf(IsNull(TBLISTA!maquina), "", TBLISTA!maquina)
            .Item(.Count).SubItems(5) = IIf(IsNull(TBLISTA!quantidade), "", Format(TBLISTA!quantidade, "###,##0.0000"))
        End With
        TBLISTA.MoveNext
        contador = contador + 1
        PBLista.Value = contador
    Loop
End If
TBLISTA.Close

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

