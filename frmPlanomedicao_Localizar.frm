Private Sub cmbFamilia_Click()
On Error GoTo tratar_erro

If cmbfamilia <> "" Then txtTexto = ""

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub cmbfiltrarpor_Click()
On Error GoTo tratar_erro

If cmbfiltrarpor = "Família" Then
    txtTexto.Visible = False
    cmbfamilia.Visible = True
Else
    txtTexto.Visible = True
    cmbfamilia.Visible = False
    If cmbfiltrarpor = "Número do controle" And txtTexto <> "" Then
        VerifNumero = txtTexto
        ProcVerificaNumero
        If VerifNumero = False Then
            txtTexto = ""
            txtTexto.SetFocus
            Exit Sub
        End If
    End If
End If

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub ProcFiltrar()
On Error GoTo tratar_erro

CamposFiltro = "M.IDPlano, M.Data, M.Inspetor, M.laudofinal, M.restricao, M.Desenho, M.Descricao, M.Fase, M.IDFase, M.ID_inspecionado, M.Peca, M.RespValidacao"
INNERJOINTEXTO = "Select " & CamposFiltro & " from Medicao M INNER JOIN projproduto P ON P.Desenho = M.Desenho LEFT JOIN item_aplicacoes IA ON IA.Codproduto = P.Codproduto"
OrdenarTexto = " order by M.idplano desc"
With frmPlanomedicao
    .ListaControle.ListItems.Clear
    If txtTexto.Visible = True And txtTexto <> "" Or cmbfamilia.Visible = True And cmbfamilia <> "" Then
        If cmbfiltrarpor = "Família" Then
            .StrSql_Controlemedicao = INNERJOINTEXTO & " where P.classe = '" & cmbfamilia & "'" & OrdenarTexto
        Else
            Select Case cmbfiltrarpor
                Case "Código interno": TextoFiltro = "M.desenho"
                Case "Código de referência": TextoFiltro = "IA.n_referencia"
                Case "Descrição": TextoFiltro = "M.descricao"
                Case "Número do controle": TextoFiltro = "M.idplano"
                Case "Número de rastreabilidade": TextoFiltro = "M.Peca"
            End Select
            .StrSql_Controlemedicao = INNERJOINTEXTO & " where " & TextoFiltro & FunVerifTipoFiltroIMF(Optinicio, Optmeio, Optfim, optIgual, txtTexto) & OrdenarTexto
        End If
    Else
        .StrSql_Controlemedicao = INNERJOINTEXTO & OrdenarTexto
    End If
    'Debug.print .StrSql_Controlemedicao
    .ProcCarregaListaControle (1)
End With
Unload Me

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
On Error GoTo tratar_erro

Select Case KeyCode
    Case vbKeyF2: ProcFiltrar
    'Case vbKeyF1: ProcAjuda
    Case vbKeyEscape: ProcSair
End Select

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

Private Sub Form_Load()
On Error GoTo tratar_erro

ProcCarregaToolBar1 Me, 9435, 5, True
ProcCarregaComboFamilia cmbfamilia, "familia <> 'Null'", True

ProcFiltroPadrao cmbfiltrarpor, Optmeio, Optfim, optIgual, 0, "Produtos/Serviços", "Q", False
If Permitido = False Then cmbfiltrarpor = "Código interno"

txtTexto.Visible = True
cmbfamilia.Visible = False

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

