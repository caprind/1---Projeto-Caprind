Private Sub ProcFiltrar()
On Error GoTo tratar_erro

TextoFiltroValidEst = ""
TextoFiltroValidProc = ""
TextoFiltroValidPlano = ""
Set TBAbrir = CreateObject("adodb.recordset")
TBAbrir.Open "Select Bloq_OP_estrutura, Bloq_OP_processo, Bloq_OP_plano from Empresa where Codigo = " & frmprod.Cmb_empresa.ItemData(frmprod.Cmb_empresa.ListIndex), Conexao, adOpenKeyset, adLockOptimistic
If TBAbrir.EOF = False Then
    If TBAbrir!Bloq_OP_estrutura = True Then TextoFiltroValidEst = " and P.DtValidacaoConj IS NOT NULL"
    If TBAbrir!Bloq_OP_processo = True Then TextoFiltroValidProc = " and PROCE.DtValidacao IS NOT NULL"
    If TBAbrir!Bloq_OP_plano = True Then TextoFiltroValidProc = " and P.DtValidacaoPlano IS NOT NULL"
End If
TBAbrir.Close
TextoFiltroValid = TextoFiltroValidEst & TextoFiltroValidProc & TextoFiltroValidPlano

CamposFiltro = "P.codProduto, P.Desenho, P.Descricao, P.Unidade, P.Classe"
INNERJOINTEXTO = "Select " & CamposFiltro & " from (Projproduto P LEFT JOIN item_aplicacoes IA ON IA.Codproduto = P.Codproduto) LEFT JOIN Processos PROCE ON PROCE.Codproduto = P.Codproduto"
If Cmb_ordenar = "Código interno" Then Ordenar = "P.desenho" Else Ordenar = "P.Descricao"
TextoFiltroPadrao = "P.Producao = 'True' and P.Bloqueado = 'False' and P.DtValidacao IS NOT NULL " & TextoFiltroValid & " group by " & CamposFiltro & " order by " & Ordenar

If txtTexto.Visible = True And txtTexto <> "" Or cmbfamilia.Visible = True And cmbfamilia <> "" Then
    If cmbfiltrarpor = "Família" Then
        StrSqlLocProdPadrao = INNERJOINTEXTO & " where classe = '" & cmbfamilia & "' and " & TextoFiltroPadrao
    Else
        Select Case cmbfiltrarpor
            Case "Código interno": TextoFiltro = "P.desenho"
            Case "Descrição": TextoFiltro = "P.descricao"
            Case "Código de referência": TextoFiltro = "IA.N_referencia"
        End Select
        StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltro & FunVerifTipoFiltroIMF(Optinicio, Optmeio, Optfim, optIgual, txtTexto) & " and " & TextoFiltroPadrao
    End If
Else
    StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltroPadrao
End If
'Debug.print StrSqlLocProdPadrao
ProcCarregaLista

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

