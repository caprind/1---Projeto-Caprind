Private Sub ProcFiltrar()
On Error GoTo tratar_erro

CamposFiltro = "P.codProduto, P.Desenho, P.Descricao, P.Unidade, P.classe"
If Cmb_ordenar = "Código interno" Then Ordenar = "P.desenho" Else Ordenar = "P.Descricao"
TextoFiltroPadrao = " P.Producao = 'True' and P.Bloqueado = 'False' and P.processo = 'False' and P.Subtipoitem <> 0 and P.Subtipoitem <> 4 group by " & CamposFiltro & " order by " & Ordenar
INNERJOINTEXTO = "Select " & CamposFiltro & " from Projproduto P LEFT JOIN item_aplicacoes IA ON IA.codproduto = P.codproduto"
If txtTexto.Visible = True And txtTexto <> "" Or cmbfamilia.Visible = True And cmbfamilia <> "" Then
    If cmbfiltrarpor = "Família" Then
        StrSqlLocProdPadrao = INNERJOINTEXTO & " where classe = '" & cmbfamilia & "' and " & TextoFiltroPadrao
    Else
        Select Case cmbfiltrarpor
            Case "Código interno": TextoFiltro = "P.desenho"
            Case "Código de referência": TextoFiltro = "IA.N_referencia"
            Case "Descrição": TextoFiltro = "P.descricao"
        End Select
        If Optinicio.Value = True Then StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltro & " like '" & txtTexto.Text & "%' and " & TextoFiltroPadrao
        If Optmeio.Value = True Then StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltro & " like '%" & txtTexto.Text & "%' and " & TextoFiltroPadrao
        If Optfim.Value = True Then StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltro & " like '%" & txtTexto.Text & "' and " & TextoFiltroPadrao
    End If
Else
    StrSqlLocProdPadrao = INNERJOINTEXTO & " where " & TextoFiltroPadrao
End If
ProcCarregaLista

Exit Sub
tratar_erro:
    USMsgBox ("Descrição do erro : " + Error()), vbCritical, "CAPRIND v5.0"
    Exit Sub
End Sub

